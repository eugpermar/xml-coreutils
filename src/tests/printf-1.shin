#!/bin/sh
# simple xml-printf test
PATH=/bin:/usr/bin
XMLPRINTF=$TESTBIN/xml-printf
TMP_PATH="`pwd`/`basename $0 .sh`_`date +"%Y%m%dT%H%M%S"`"

mkdir "$TMP_PATH"

$XMLPRINTF "%s" cat-1.verify:/root/abc > "$TMP_PATH/output"

diff "$TMP_PATH/output" printf-1.verify

RESULT=$?
rm -rf "$TMP_PATH"
exit $RESULT
