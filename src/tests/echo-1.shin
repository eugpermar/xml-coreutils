#!/bin/sh
# check xml-echo Hello world
PATH=/bin:/usr/bin
XMLECHO=$TESTBIN/xml-echo
TMP_PATH="`pwd`/`basename $0 .sh`_`date +"%Y%m%dT%H%M%S"`"

mkdir "$TMP_PATH"

$XMLECHO -e "[abc][onetwothree]easy[../do]simple[../re]simple[../mi]simple" > "$TMP_PATH/output"

diff echo-1.verify "$TMP_PATH/output"

RESULT=$?
rm -rf "$TMP_PATH"
exit $RESULT
