#!/bin/sh
# check that xml-strings removes tags
PATH=/bin:/usr/bin
XMLSTRINGS=$TESTBIN/xml-strings
TMP_PATH="`pwd`/`basename $0 .sh`_`date +"%Y%m%dT%H%M%S"`"

mkdir "$TMP_PATH"

$XMLSTRINGS cat-1.verify > "$TMP_PATH/output"

diff "$TMP_PATH/output" strings-1.verify

RESULT=$?
rm -rf "$TMP_PATH"
exit $RESULT
